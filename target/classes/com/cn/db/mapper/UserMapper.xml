<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cn.db.mapper.UserMapper">

    <select id="getAllUsers" resultType="com.cn.db.entity.User">
        SELECT user_id, login_name, full_name, birthday, phone, email FROM user_accounts;
    </select>
    <delete id="deleteUserById" parameterType="int">
        DELETE FROM user_accounts WHERE user_id = #{userId}
    </delete>
</mapper>
